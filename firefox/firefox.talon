app: firefox
-

tab (next | right): key("ctrl-pagedown")

tab (previous | left): key("ctrl-pageup")

first tab: key("ctrl-1")

second tab: key("ctrl-2")

third tab: key("ctrl-3")

fourth tab: key("ctrl-4")

fifth tab: key("ctrl-5")

new tab: key("ctrl-t")

close tab: key("ctrl-w")

reopen tab: key("ctrl-shift-t")
